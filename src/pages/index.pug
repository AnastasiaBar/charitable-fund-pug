extends ../templates/layout_default.pug

block variables
  - isMainPage = false
  - pageTitle = 'Стартовая страница'
  - metaDescription = 'description'
  - metaKeywords = 'keywords'

block content

  -
    var slidersMain = [
      {
        title: '2 605 детей',
        subTitle: 'за все время работы фонда получили помощь и возможность заниматься спортом',
        slideMain: '../img/hero__image-01.png',
        galleryPhoto1: '../img/hero__image-02.png',
        galleryPhoto2: '../img/hero__image-03.png',
        colorSlide: '#B3315E',
        details: true
      },
      {
        title: 'Пас в будущее',
        subTitle: 'Наша цель – дать возможность талантливым ребятам, увлеченным спортом, проявить свои способности',
        slideMain: '../img/hero__image-02.png',
        galleryPhoto1: '../img/hero__image-03.png',
        galleryPhoto2: '../img/hero__image-01.png',
        colorSlide: '#dc9138',
        details: false
      },
      {
        smallHeadline: 'Фонд передал футбольную площадку Центру помощи детям «Ручеёк»',
        subTitle: '',
        slideMain: '../img/hero__image-03.png',
        galleryPhoto1: '../img/hero__image-01.png',
        galleryPhoto2: '../img/hero__image-02.png',
        colorSlide: '#067256',
        details: true
      }
    ]

